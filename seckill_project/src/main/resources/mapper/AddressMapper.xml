<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace= "com.zlt.dao.AddressMapper" >


    <insert id="insertAddress" parameterType="com.zlt.domain.Address">
        insert into address(addressId,receiverName,receiverAddress,receiverPhone,postNo,userId)
        values(#{addressId},#{receiverName},#{receiverAddress},#{receiverPhone},#{postNo},#{userId})
    </insert>



</mapper>